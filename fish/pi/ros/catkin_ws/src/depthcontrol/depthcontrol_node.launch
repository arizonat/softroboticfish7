<launch>

    <node pkg="depthcontrol" type="depth_control.py" name="depth_control"/>


    <!-- joy node -->
    <node respawn="true" pkg="joy" type="joy_node" name="joy" output="screen" >
          <param name="dev" type="string" value="dev/input/js0" />
          <param name="deadzone" value="0.1" />
    </node>

</launch>
